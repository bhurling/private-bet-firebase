{
  "rules": {
    ".read": "auth.uid === 'feeds-worker'",
    "bets": {
      "$betId": {
        ".read": "data.child('users').hasChild(auth.uid)",
        ".write": "data.child('users').hasChild(auth.uid)"
      }
    },
    "feeds": {
      ".write": "auth.uid === 'feeds-worker'",
      "$uid": {
        ".read": "$uid === auth.uid"
      }
    },
    "categories": {
      ".read": "auth !== null"
    },
    "profiles": {
      "$uid": {
        ".write": "$uid === auth.uid",
        "public": {
          ".read": "auth !== null"
        },
        "private": {
          ".read": "$uid === auth.uid"
        }
      }
    },
    "links": {
      "$uid": {
        "incoming": {
          ".read": "$uid === auth.uid",
          "$oid": {
            ".write": "$oid === auth.uid",
            ".validate": "newData.isBoolean()"
          }
        }
      }
    }
  }
}
